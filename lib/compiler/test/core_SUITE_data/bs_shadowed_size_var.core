%% Reduced code from beam_asm inlined using the old inliner.

'filter_essentials'/1 =
    fun (_cor0) ->
	case _cor0 of
	  <[#{#<Sz>(32,1,'integer',['unsigned','big']) }#|T]> when 'true' ->
	      let <_cor4> =
                      case T of
			%% Variable 'Sz' repeated here. Should work.
			<#{#<Sz>(32,1,'integer',['unsigned','big']),
			   #<Tail>('all',1,'binary',['unsigned','big'])}#> when 'true' ->
			   case Tail of
			       <#{#<Data>(Sz,8,'binary',['unsigned','big'])}#> when 'true' ->
			         Data
			       <_cor5> when 'true' ->
			           primop 'match_fail'
				      ({'case_clause',{_cor5}})
		           end
			<_cor5> when 'true' ->
			    primop 'match_fail'
				({'case_clause',{_cor5}})
		      end
	      in  [Sz|_cor4]
	  <_cor5> when 'true' ->
	      primop 'match_fail'
		  ({'function_clause',_cor5})
	end
end
