//
// Checking that default labels are correct in TK
//

interface i1 {
  union U1 switch (long) {
  default: long a;
  case 1: case 2: long b;
  };

  union U2 switch (long) {
  case 0: default: long a;
  case 1: case 2: long b;
  };

  union U3 switch (long) {
  case -1: long aa;
  case 0: default: long a;
  case 1: case 2: long b;
  };
  
  union U4 switch (long) {
  case -1: long aa;
  case 0: long a;
  case 1: case 2: long b;
  };
  
  U1 op0();
  U2 op1();
  U3 op2();
  U4 op3();
};
